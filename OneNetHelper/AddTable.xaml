<Window x:Class="OneNetHelper.AddTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneNetHelper"
        mc:Ignorable="d"
        Title="AddTable" Height="300" Width="300" ResizeMode="CanMinimize">
    <Grid>
        <Label Content="参数名：" HorizontalAlignment="Left" Margin="35,20,0,0" VerticalAlignment="Top" />
        <Label Content="参数值：" HorizontalAlignment="Left" Margin="35,105,0,0" VerticalAlignment="Top" />
        <ComboBox x:Name="Comb_Para_Name" HorizontalAlignment="Left" Height="23" Margin="50,64,0,0"  VerticalAlignment="Top" Width="200"/>
        <TextBox x:Name="Text_Para_Vaule" HorizontalAlignment="Left" Height="23" Margin="50,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200"/>
        <Button Content="保存参数" HorizontalAlignment="Left" Margin="128,216,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
